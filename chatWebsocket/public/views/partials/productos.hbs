<table>
  <thead>
    <tr>
      <th>Producto</th>
      <th>Precio</th>
      <th>Imagen</th>
    </tr>
  </thead>
  <tbody>
    {{#each items}}
      <tr>
        <td>{{this.title}}</td><td>{{this.price}}</td><td><img
            class="thumbnail"
            src={{this.thumbnail}}
          /></td>
      </tr>
    {{/each}}
  </tbody>
</table>